<?xml version="1.0" encoding="utf-8"?>
<VSTemplate Version="3.0.0" Type="Project" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005" xmlns:sdk="http://schemas.microsoft.com/developer/vstemplate-sdkextension/2010">
  <TemplateData>
    <Name>RhinoCommon Plug-In for Rhinoceros 6 (C#)</Name>
    <Description>A new RhinoCommon plug-in project that can be easily debugged and can automatically launch Rhino</Description>
    <Icon>icon.ico</Icon>
    <ProjectType>CSharp</ProjectType>
    <RequiredFrameworkVersion>4.5</RequiredFrameworkVersion>
    <SortOrder>1000</SortOrder>
    <TemplateID>7ADB6134-BEBD-4561-8485-56269DF8FB74</TemplateID>
    <CreateNewFolder>true</CreateNewFolder>
    <DefaultName>CSProjectTemplate</DefaultName>
    <ProvideDefaultName>true</ProvideDefaultName>
    <LanguageTag>csharp</LanguageTag>
    <PlatformTag>windows</PlatformTag>
    <ProjectTypeTag>desktop</ProjectTypeTag>
  </TemplateData>
  <TemplateContent>
    <Project File="ProjectTemplate.csproj" ReplaceParameters="true">
      <ProjectItem ReplaceParameters="true" TargetFileName="Properties\AssemblyInfo.cs">AssemblyInfo.cs</ProjectItem>
      <ProjectItem ReplaceParameters="true" OpenInEditor="true" OpenOrder="1">$commandname$.cs</ProjectItem>
      <ProjectItem ReplaceParameters="true" OpenInEditor="true" OpenOrder="2">$pluginname$.cs</ProjectItem>
      
      <ProjectItem ReplaceParameters="false" TargetFileName="EmbeddedResources\plugin-digitize.ico" OpenInEditor="false">plugin-digitize.ico</ProjectItem>
      <ProjectItem ReplaceParameters="false" TargetFileName="EmbeddedResources\plugin-export.ico" OpenInEditor="false">plugin-export.ico</ProjectItem>
      <ProjectItem ReplaceParameters="false" TargetFileName="EmbeddedResources\plugin-import.ico" OpenInEditor="false">plugin-import.ico</ProjectItem>
      <ProjectItem ReplaceParameters="false" TargetFileName="EmbeddedResources\plugin-render.ico" OpenInEditor="false">plugin-render.ico</ProjectItem>
      <ProjectItem ReplaceParameters="false" TargetFileName="EmbeddedResources\plugin-utility.ico" OpenInEditor="false">plugin-utility.ico</ProjectItem>
    </Project>
  </TemplateContent>
  <WizardExtension>
    <Assembly>NewRhinoCommonTemplate, Version=1.2.2.0, Culture=Neutral, PublicKeyToken=9cbe2d392744cfe6</Assembly>
    <FullClassName>NewRhinoCommonTemplate.CollectInformationWizard</FullClassName>
  </WizardExtension>
  
</VSTemplate>